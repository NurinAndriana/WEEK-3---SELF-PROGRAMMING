		double average;
		double Div;
		double number6 = 66;
		double number7 = 88;
		float number8 = 99;
		float number9 = 200;
		float Div2;		
		double mul;
		double mul2;
		
		
		
		
		mul2 = (number1*number2)/number3;
		sum = number1 + number2 + number3 + number4 + number5;
